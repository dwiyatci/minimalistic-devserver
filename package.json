{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver powered by webpack 5",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <gmd@email.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "up": "yarn update",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack serve",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "lint": "eslint --cache .",
    "lint-print-config": "eslint --print-config src/index.js",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.15.3",
    "core-js": "3.16.1",
    "jquery": "3.6.0",
    "lodash": "4.17.21",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-object-rest-spread": "7.14.7",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "css-loader": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "imports-loader": "3.0.0",
    "inline-manifest-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "2.2.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "style-loader": "3.2.1",
    "url-loader": "4.1.1",
    "webpack": "5.50.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
