{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v2.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade",
    "update": "updtr",
    "start": "webpack-dev-server --hot",
    "build": "npm run lint && npm run clean && webpack --env=PROD --colors",
    "lint": "eslint --cache src",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "jquery": "3.1.1",
    "lodash": "4.17.2"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "css-loader": "0.26.1",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "imports-loader": "0.7.0",
    "json-loader": "0.5.4",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "updtr": "0.2.3",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.28",
    "webpack-dev-server": "2.1.0-beta.12",
    "yarn": "0.18.1"
  }
}
