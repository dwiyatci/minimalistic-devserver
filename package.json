{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v4.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack-dev-server --hot",
    "build": "yarn webpack --env.production --progress",
    "webpack": "yarn clean && webpack",
    "clean": "rimraf dist",
    "lint": "eslint --cache src",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/runtime-corejs2": "7.2.0",
    "jquery": "3.3.1",
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "css-loader": "2.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.0.0-beta.2",
    "imports-loader": "0.8.0",
    "inline-manifest-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "0.5.0",
    "prettier": "1.15.3",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.1",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  }
}
