{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v2.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server",
    "cometd playground"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "rimraf node_modules && npm install",
    "update": "updtr -e jquery,lodash,q",
    "start": "webpack-dev-server --hot",
    "build": "npm run clean && webpack --env.prod",
    "lint": "eslint --cache src",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "jquery": "2.1.1",
    "lodash": "2.4.1",
    "q": "1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2015-webpack": "6.4.2",
    "babel-preset-stage-2": "6.13.0",
    "eslint": "3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "6.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "rimraf": "2.5.4",
    "updtr": "0.2.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "2.1.0-beta.0"
  }
}
