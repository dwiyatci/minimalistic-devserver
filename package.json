{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v4.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <gmd@email.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack serve",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "lint": "eslint --cache .",
    "lint-print-config": "eslint --print-config src/index.js",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.12.1",
    "jquery": "3.5.1",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "babel-loader": "8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "imports-loader": "1.2.0",
    "inline-manifest-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "1.2.1",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.3.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0"
  }
}
