{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v4.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack-dev-server --hot",
    "build": "yarn webpack --env.production --progress",
    "webpack": "yarn clean && webpack",
    "clean": "rimraf dist",
    "lint": "eslint --cache .",
    "lint-print-config": "eslint --print-config src/index.js",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.6.2",
    "core-js": "3.2.1",
    "jquery": "3.4.1",
    "lodash": "4.17.15",
    "regenerator-runtime": "0.13.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "css-loader": "3.2.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.0.0-beta.8",
    "imports-loader": "0.8.0",
    "inline-manifest-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "0.8.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "url-loader": "2.2.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  }
}
