{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v2.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive",
    "start": "webpack-dev-server",
    "webpack": "npm run clean && webpack",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "lint": "eslint --cache src",
    "clean": "rimraf assets"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.23.0",
    "jquery": "3.2.1",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.29.0",
    "imports-loader": "0.7.1",
    "inline-manifest-webpack-plugin": "3.0.1",
    "json-loader": "0.5.4",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0"
  }
