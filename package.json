{
  "name": "minimalistic-devserver",
  "version": "0.0.0-semantically-released",
  "description": "minimalistic devserver using webpack-dev-server v2.x",
  "keywords": [
    "minimalist",
    "devserver",
    "development server"
  ],
  "homepage": "https://github.com/dwiyatci/minimalistic-devserver",
  "bugs": "https://github.com/dwiyatci/minimalistic-devserver/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/minimalistic-devserver.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive",
    "start": "webpack-dev-server",
    "webpack": "npm run clean && webpack",
    "build": "npm run lint && npm run clean && cross-env NODE_ENV=production webpack",
    "lint": "eslint --cache src",
    "clean": "rimraf assets"
  },
  "dependencies": {
    "jquery": "1.9.1"
  },
  "devDependencies": {
    "cross-env": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.1",
    "script-loader": "0.7.0",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5"
  }
}
